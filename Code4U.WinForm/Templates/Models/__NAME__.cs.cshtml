@using System
@using System.Collections.Generic
@using System.Linq
@using System.Text
@using System.IO
@using RazorEngine
@using RazorEngine.Templating
@using DatabaseSchemaReader
@using DatabaseSchemaReader.DataSchema

@inherits RazorEngine.Templating.Code4UTemplateBase< DatabaseSchemaReader.DataSchema.DatabaseTable >
using System;
using System.Collections.Generic;
using System.Linq;

namespace Models
{
    public class @Model.Name
    {
    @foreach (var column in @Model.Columns)
    {
        @:@IncludeInLine("_Property.cshtml", column, typeof(DatabaseSchemaReader.DataSchema.DatabaseColumn))
        @:
    }
    }
}
