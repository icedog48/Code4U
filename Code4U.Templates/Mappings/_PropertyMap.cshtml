@using System
@using System.Collections.Generic
@using System.Linq
@using System.Text
@using System.IO
@using RazorEngine
@using RazorEngine.Templating
@using Code4U.Models

@inherits RazorEngine.Templating.Code4UTemplateBase<Property>

@if (Model.HasFlag("PrimaryKey"))
{
@:Id(x => x.@Model.Name);
}
else if (Model.HasFlag("ForeignKey"))
{
@:References(x => x.@Model.Name);
}
else if (Model.HasFlag("Collection"))
{
@:HasMany(x => x.@Model.Name);
}
else
{
@:Map(x => x.@Model.Name);
}
